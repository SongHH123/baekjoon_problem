    "import math\n",
    "while(True):\n",
    "    i = str(int(input()))\n",
    "    if(i=='0'):\n",
    "        break\n",
    "    for j in range(math.floor(len(i)/2)):\n",
    "        if(i[j]!=i[-(j+1)]):\n",
    "            j=-1\n",
    "            break\n",
    "\n",
    "    if(j == int(len(i)/2)-1 or len(i)==1):\n",
    "        print('yes')\n",
    "    else:\n",
    "        print('no')"
   ]
  }
