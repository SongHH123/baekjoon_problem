    "#평균은 넘겠지\n",
    "C = int(input())\n",
    "Rate = []\n",
    "\n",
    "for i in range(C):\n",
    "    Rate.append(0)\n",
    "    NG = input().split(' ')\n",
    "    NG = list(map(int, NG))\n",
    "    N = NG[0]\n",
    "    G = NG[1:]\n",
    "    Sum = 0\n",
    "    avg = sum(G)/N\n",
    "    for j in range(N):\n",
    "        if(G[j]>avg):\n",
    "            Rate[i] += 1\n",
    "    Rate[i] = round((Rate[i]/N)*100, 3)\n",
    "\n",
    "for i in range(len(Rate)):\n",
    "    a = Rate[i]\n",
    "    print(str(format(a, \".3f\"))+\"%\")"
   ]
